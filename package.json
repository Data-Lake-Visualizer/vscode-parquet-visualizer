{
  "name": "parquet-visualizer",
  "displayName": "parquet-visualizer",
  "description": "Visualizes Apache Parquet files in table format.",
  "publisher": "lucien-martijn",
  "icon": "media/ico128.png",
  "homepage": "https://github.com/luusluus/vscode-parquet-visualizer",
  "bugs": {
    "url": "https://github.com/luusluus/vscode-parquet-visualizer/issues",
    "email": "lucienmartijn@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luusluus/vscode-parquet-visualizer.git"
  },
  "author": {
    "name": "Lucien Martijn",
    "email": "lucienmartijn@gmail.com",
    "url": "https://github.com/luusluus"
  },
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0",
    "node": ">18.0.0"
  },
  "categories": [
    "Visualization",
    "Data Science",
    "Formatters"
  ],
  "keywords": [
    "parquet",
    "visualizer",
    "table",
    "schema"
  ],
  "activationEvents": [],
  "main": "./dist/extension",
  "contributes": {
    "customEditors": [
      {
        "viewType": "parquet-visualizer.parquetVisualizer",
        "displayName": "Parquet Visualizer",
        "selector": [
          {
            "filenamePattern": "*.parquet"
          }
        ]
      }
    ],
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "tsc --build",
    "build": "tsc --build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "esbuild": "0.20.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@vscode-logging/logger": "^1.2.3",
    "@vscode-logging/wrapper": "^1.0.1",
    "apache-arrow": "^15.0.1",
    "parcel": "^2.10.3",
    "parquet-wasm": "^0.5.0"
  }
}
