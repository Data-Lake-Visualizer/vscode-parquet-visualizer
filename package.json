{
    "name": "parquet-visualizer",
    "displayName": "Parquet Visualizer",
    "description": "Inspect and query very large parquet/CSV files fast and easy",
    "publisher": "lucien-martijn",
    "icon": "media/ico128.png",
    "homepage": "https://github.com/Data-Lake-Visualizer/vscode-parquet-visualizer",
    "bugs": {
        "url": "https://github.com/Data-Lake-Visualizer/vscode-parquet-visualizer/issues",
        "email": "lucienmartijn@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Data-Lake-Visualizer/vscode-parquet-visualizer.git"
    },
    "author": {
        "name": "Lucien Martijn",
        "email": "lucienmartijn@gmail.com",
        "url": "https://github.com/luusluus"
    },
    "license": "MIT",
    "version": "0.10.1",
    "engines": {
        "vscode": "^1.74.0",
        "node": ">18.0.0"
    },
    "categories": [
        "Visualization",
        "Data Science",
        "Formatters",
        "Other"
    ],
    "keywords": [
        "parquet",
        "csv",
        "visualizer",
        "table",
        "schema",
        "SQL",
        "duckdb",
        "query"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "parquet-visualizer",
                    "title": "Parquet Visualizer",
                    "icon": ""
                }
            ]
        },
        "views": {
            "parquet-visualizer": [
                {
                    "id": "dataLakeExlorerView",
                    "name": "Data Lake Explorer",
                    "type": "tree",
                    "contextualTitle": "Data Lake Explorer"
                },
                {
                    "id": "accountView",
                    "name": "Account",
                    "type": "tree"
                }
            ]
        },
        "commands": [
            {
                "command": "dataLakeExlorerView.refresh",
                "title": "Refresh",
                "icon": {
                    "light": "resources/icons/vscode/light/refresh.svg",
                    "dark": "resources/icons/vscode/dark/refresh.svg"
                }
            },
            {
                "command": "dataLakeExlorerView.aws.s3.refresh",
                "title": "Refresh",
                "icon": {
                    "light": "resources/icons/vscode/light/refresh.svg",
                    "dark": "resources/icons/vscode/dark/refresh.svg"
                }
            },
            {
                "command": "dataLakeExlorerView.aws.s3Connection.selectConnection",
                "title": "Select AWS Connection"
            },
            {
                "command": "dataLakeExlorerView.aws.s3Connection.addConnection",
                "title": "Add New Connection"
            },
            {
                "command": "dataLakeExlorerView.aws.s3Connection.switchConnection",
                "title": "Switch Connection"
            },
            {
                "command": "dataLakeExlorerView.aws.s3Connection.signOut",
                "title": "Sign Out"
            },
            {
                "command": "dataLakeExlorerView.aws.openS3Parquet",
                "title": "Open Parquet File"
            }
            
        ],
        "menus": {
            "view/title": [
                {
                    "command": "dataLakeExlorerView.refresh",
                    "when": "view == dataLakeExlorerView",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "dataLakeExlorerView.aws.s3.refresh",
                    "when": "view == dataLakeExlorerView && viewItem == aws",
                    "group": "inline"
                },
                {
                    "submenu": "dataLakeExlorerView.aws.s3Connection",
                    "when": "view == dataLakeExlorerView && viewItem == AWSConnectionItem",
                    "group": "inline"
                }
            ],
            "dataLakeExlorerView.aws.s3Connection": [
                {
                    "command": "dataLakeExlorerView.aws.s3Connection.switchConnection",
                    "group": "0@1"
                },
                {
                    "command": "dataLakeExlorerView.aws.s3Connection.signOut",
                    "group": "0@2"
                }
            ]
        },
        "submenus": [
            {
                "id": "dataLakeExlorerView.aws.s3Connection",
                "label": "Authentication",
                "icon": "$(ellipsis)",
                "when": "view == dataLakeExlorerView && viewItem == AWSConnectionItem"
            }
        ],
        "configuration": {
            "title": "parquet-visualizer",
            "properties": {
                "parquet-visualizer.backend": {
                    "markdownDescription": "Choose a backend for reading the parquet file.",
                    "type": "string",
                    "enum": [
                        "duckdb",
                        "parquet-wasm"
                    ],
                    "default": "duckdb"
                },
                "parquet-visualizer.defaultQuery": {
                    "type": "string",
                    "editPresentation": "multilineText",
                    "default": "SELECT *\r\nFROM data\r\nLIMIT 1000;",
                    "markdownDescription": "Default SQL query for parquet file. The table `data` should remain the same."
                },
                "parquet-visualizer.RunQueryKeyBinding": {
                    "type": "string",
                    "default": "Ctrl-Enter",
                    "markdownDescription": "Default Key Binding for running queries. If Ctrl is written, it will be translated to Command for mac and vica versa. E.g., Ctrl-E will be synonymous to Command-E."
                },
                "parquet-visualizer.defaultPageSizes": {
                    "markdownDescription": "Set the default page size for data and query tab.",
                    "type": [
                        "array"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "default": [
                        "20",
                        "All"
                    ]
                },
                "parquet-visualizer.dateTimeFormat": {
                    "markdownDescription": "Set datetime format for columns of timestamp type. Defaults to ISO8601. You can set a custom format like `YYYY-MM-DD HH:mm:ss.SSS Z`. Find rules for formatting [here](https://www.npmjs.com/package/date-and-time#formatdateobj-arg-utc).",
                    "type": "string",
                    "default": "ISO8601"
                },
                "parquet-visualizer.outputDateTimeFormatInUTC": {
                    "type": "boolean",
                    "markdownDescription": "Outputs the datetime format for timestamp columns in UTC or in local time.",
                    "default": true
                },
                "parquet-visualizer.logging.panel": {
                    "type": "boolean",
                    "markdownDescription": "Write diagnostic logs to an output panel.",
                    "default": false
                },
                "parquet-visualizer.logging.folder": {
                    "type": "string",
                    "markdownDescription": "Write diagnostic logs under the given directory."
                },
                "parquet-visualizer.logging.level": {
                    "type": "string",
                    "enum": [
                        "off",
                        "fatal",
                        "error",
                        "warn",
                        "info",
                        "debug",
                        "trace"
                    ],
                    "default": "info",
                    "markdownDescription": "Log level."
                }
            }
        },
        "customEditors": [
            {
                "viewType": "parquet-visualizer.parquetVisualizer",
                "displayName": "Parquet Visualizer",
                "selector": [
                    {
                        "filenamePattern": "*.parquet"
                    },
                    {
                        "filenamePattern": "*.parq"
                    },
                    {
                        "filenamePattern": "*.pq"
                    },
                    {
                        "filenamePattern": "*.csv"
                    }
                ]
            }
        ]
    },
    "scripts": {
        "package": "node build.js",
        "vscode:prepublish": "npm run package",
        "build": "tsc --build tsconfig.dev.json && npm run copy-resources ",
        "copy-resources": "rsync -av resources out",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/emscripten": "^1.39.12",
        "@types/glob": "^8.1.0",
        "@types/ini": "^4.1.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.12.12",
        "@types/vscode": "^1.73.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.2",
        "esbuild": "^0.18.17",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "prettier": "3.4.2",
        "semantic-release": "^24.2.0",
        "semantic-release-stop-before-publish": "^1.0.0",
        "semantic-release-vsce": "^5.7.4",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.735.0",
        "@aws-sdk/client-sts": "^3.735.0",
        "@vscode-logging/logger": "^2.0.0",
        "@vscode-logging/wrapper": "^2.0.0",
        "@vscode/extension-telemetry": "^0.9.7",
        "apache-arrow": "^15.0.0",
        "comlink": "^4.4.2",
        "date-and-time": "^3.6.0",
        "duckdb-async": "^1.1.3",
        "exceljs": "^4.4.0",
        "ini": "^5.0.0",
        "parquet-wasm": "^0.6.0",
        "vscode-uri": "^3.0.8"
    },
    "binary": {
        "module_name": "duckdb",
        "module_path": "./out/binding/",
        "host": "https://duckdb-node.s3.amazonaws.com"
    }
}
