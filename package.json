{
  "name": "parquet-visualizer",
  "displayName": "parquet-visualizer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "webview/context": [
        {
          "command": "parquet-visualizer.yarn",
          "when": "webviewId == 'parquet-visualizer'"
        },
        {
          "command": "parquet-visualizer.insertLion",
          "when": "webviewId == 'parquet-visualizer' && webviewSection == 'editor'"
        }
      ]
    },
    "commands": [
      {
        "command": "parquet-visualizer.start",
        "title": "Start new cat coding session",
        "category": "Cat Coding"
      },
      {
        "command": "parquet-visualizer.doRefactor",
        "title": "Do some refactoring",
        "category": "Cat Coding"
      },
      {
        "command": "parquet-visualizer.yarn",
        "title": "Yarn üß∂",
        "category": "Cat Coding"
      },
      {
        "command": "parquet-visualizer.insertLion",
        "title": "Insert ü¶Å",
        "category": "Cat Coding"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@dvirtz/parquets": "^0.11.6"
  }
}
