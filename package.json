{
  "name": "parquet-visualizer",
  "displayName": "parquet-visualizer",
  "description": "Visualizes Apache Parquet files in table format.",
  "publisher": "lucien-martijn",
  "icon": "media/ico128.png",
  "homepage": "https://github.com/luusluus/vscode-parquet-visualizer",
  "bugs": {
    "url": "https://github.com/luusluus/vscode-parquet-visualizer/issues",
    "email": "lucienmartijn@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luusluus/vscode-parquet-visualizer.git"
  },
  "author": {
    "name": "Lucien Martijn",
    "email": "lucienmartijn@gmail.com",
    "url": "https://github.com/luusluus"
  },
  "license": "MIT",
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.74.0",
    "node": ">18.0.0"
  },
  "categories": [
    "Visualization",
    "Data Science",
    "Formatters",
    "Other"
  ],
  "keywords": [
    "parquet",
    "visualizer",
    "table",
    "schema",
    "SQL",
    "duckdb",
    "query"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "parquet-visualizer.parquetVisualizer",
        "displayName": "Parquet Visualizer",
        "selector": [
          {
            "filenamePattern": "*.parquet"
          },
          {
            "filenamePattern": "*.pq"
          }
        ]
      }
    ],
    "commands": []
  },
  "scripts": {
    "compile": "webpack --mode development",
    "package": "webpack --mode production --devtool hidden-source-map",
    "vscode:prepublish": "npm run package",
    "build": "tsc --build",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/emscripten": "^1.39.12",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.12.12",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "aws-sdk": "^2.1658.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "html-loader": "^5.0.0",
    "json-loader": "^0.5.7",
    "mocha": "^10.2.0",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.5.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.1.3",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "apache-arrow": "^15.0.0",
    "duckdb-async": "^1.0.0",
    "parquet-wasm": "^0.6.0",
    "webpack-node-externals": "^3.0.0"
  }
}
